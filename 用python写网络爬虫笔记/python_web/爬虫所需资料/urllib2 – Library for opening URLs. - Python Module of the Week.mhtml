   =20
    <span class=3D"k">def</span> <span class=3D"nf">__str__</span><span cla=
ss=3D"p">(</span><span class=3D"bp">self</span><span class=3D"p">):</span>
        <span class=3D"sd">"""Return a string representing the form data, i=
ncluding attached files."""</span>
        <span class=3D"c"># Build a list of lists, each containing "lines" =
of the</span>
        <span class=3D"c"># request.  Each part is separated by a boundary =
string.</span>
        <span class=3D"c"># Once the list is built, return a string where e=
ach</span>
        <span class=3D"c"># line is separated by '\r\n'.  </span>
        <span class=3D"n">parts</span> <span class=3D"o">=3D</span> <span c=
lass=3D"p">[]</span>
        <span class=3D"n">part_boundary</span> <span class=3D"o">=3D</span>=
 <span class=3D"s">'--'</span> <span class=3D"o">+</span> <span class=3D"bp=
">self</span><span class=3D"o">.</span><span class=3D"n">boundary</span>
       =20
        <span class=3D"c"># Add the form fields</span>
        <span class=3D"n">parts</span><span class=3D"o">.</span><span class=
=3D"n">extend</span><span class=3D"p">(</span>
            <span class=3D"p">[</span> <span class=3D"n">part_boundary</spa=
n><span class=3D"p">,</span>
              <span class=3D"s">'Content-Disposition: form-data; name=3D"</=
span><span class=3D"si">%s</span><span class=3D"s">"'</span> <span class=3D=
"o">%</span> <span class=3D"n">name</span><span class=3D"p">,</span>
              <span class=3D"s">''</span><span class=3D"p">,</span>
              <span class=3D"n">value</span><span class=3D"p">,</span>
            <span class=3D"p">]</span>
            <span class=3D"k">for</span> <span class=3D"n">name</span><span=
 class=3D"p">,</span> <span class=3D"n">value</span> <span class=3D"ow">in<=
/span> <span class=3D"bp">self</span><span class=3D"o">.</span><span class=
=3D"n">form_fields</span>
            <span class=3D"p">)</span>
       =20
        <span class=3D"c"># Add the files to upload</span>
        <span class=3D"n">parts</span><span class=3D"o">.</span><span class=
=3D"n">extend</span><span class=3D"p">(</span>
            <span class=3D"p">[</span> <span class=3D"n">part_boundary</spa=
n><span class=3D"p">,</span>
              <span class=3D"s">'Content-Disposition: file; name=3D"</span>=
<span class=3D"si">%s</span><span class=3D"s">"; filename=3D"</span><span c=
lass=3D"si">%s</span><span class=3D"s">"'</span> <span class=3D"o">%</span>=
 \
                 <span class=3D"p">(</span><span class=3D"n">field_name</sp=
an><span class=3D"p">,</span> <span class=3D"n">filename</span><span class=
=3D"p">),</span>
              <span class=3D"s">'Content-Type: </span><span class=3D"si">%s=
</span><span class=3D"s">'</span> <span class=3D"o">%</span> <span class=3D=
"n">content_type</span><span class=3D"p">,</span>
              <span class=3D"s">''</span><span class=3D"p">,</span>
              <span class=3D"n">body</span><span class=3D"p">,</span>
            <span class=3D"p">]</span>
            <span class=3D"k">for</span> <span class=3D"n">field_name</span=
><span class=3D"p">,</span> <span class=3D"n">filename</span><span class=3D=
"p">,</span> <span class=3D"n">content_type</span><span class=3D"p">,</span=
> <span class=3D"n">body</span> <span class=3D"ow">in</span> <span class=3D=
"bp">self</span><span class=3D"o">.</span><span class=3D"n">files</span>
            <span class=3D"p">)</span>
       =20
        <span class=3D"c"># Flatten the list and add closing boundary marke=
r,</span>
        <span class=3D"c"># then return CR+LF separated data</span>
        <span class=3D"n">flattened</span> <span class=3D"o">=3D</span> <sp=
an class=3D"nb">list</span><span class=3D"p">(</span><span class=3D"n">iter=
tools</span><span class=3D"o">.</span><span class=3D"n">chain</span><span c=
lass=3D"p">(</span><span class=3D"o">*</span><span class=3D"n">parts</span>=
<span class=3D"p">))</span>
        <span class=3D"n">flattened</span><span class=3D"o">.</span><span c=
lass=3D"n">append</span><span class=3D"p">(</span><span class=3D"s">'--'</s=
pan> <span class=3D"o">+</span> <span class=3D"bp">self</span><span class=
=3D"o">.</span><span class=3D"n">boundary</span> <span class=3D"o">+</span>=
 <span class=3D"s">'--'</span><span class=3D"p">)</span>
        <span class=3D"n">flattened</span><span class=3D"o">.</span><span c=
lass=3D"n">append</span><span class=3D"p">(</span><span class=3D"s">''</spa=
n><span class=3D"p">)</span>
        <span class=3D"k">return</span> <span class=3D"s">'</span><span cla=
ss=3D"se">\r\n</span><span class=3D"s">'</span><span class=3D"o">.</span><s=
